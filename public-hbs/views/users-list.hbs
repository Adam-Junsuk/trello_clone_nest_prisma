{{! views/users-list.hbs }}
<table class='table'>
  <thead>
    <tr>
      <th scope='col'>#</th>
      <th scope='col'>이름</th>
      <th scope='col'>이메일</th>
    </tr>
  </thead>
  <tbody>
    {{#each users}}
      <tr>
        <th scope='row'>{{this.userId}}</th>
        <td>{{this.username}}</td>
        <td>{{this.email}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>